#include<stdio.h>
int main()
{
	int n,i;
	printf("enter  the number of elements:");
	scanf("%d",&n);
	int arr[n];
	printf("enter the elements:");
	for(i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
	}
	printf("odd numbers:");
	for(i=0;i<n;i++)
	{
		if(arr[i]%2!=0)
		{
			printf("%d",arr[i]);
		}
	}
	printf("\neven numbers:");
	for(i=0;i<n;i++)
	{
		if(arr[i]%2==0)
		{
			printf("%d",arr[i]);
		}
	}
	return 0;
}
